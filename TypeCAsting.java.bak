class TypeCAsting 
{
	public static void main(String[] args) 
	{
		int a = 10; 
		long l = a;
		float f = a;
		double d = a;
		char c = a;
		System.out.println(f);System.out.println(l);System.out.println(d);System.out.println(c);
		
		long l1 = 20;
		int a1 = (int)l1;
		byte b1 = (byte)a1;
		short s1 = (byte)b1;
		System.out.println(a1);System.out.println(b1);System.out.println(s1);
	}
}
